<form method="post" action="{{ path('page_delete', {'id': page.id}) }}" onsubmit="return confirm('Etes-vous certain de vouloir supprimer cette page ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ page.id) }}">
    <button class="btn btn-sm btn-danger">Supprimer</button>
</form>
